    "build": "shadow-cljs compile server client",
    "release": "shadow-cljs release server client"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "shadow-cljs": "2.15.8"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "volta": {
    "node": "16.8.0",
    "yarn": "1.22.11"
  }
}
